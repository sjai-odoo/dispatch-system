<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.transport</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//setting[@id='product_packs_tracking']" position="after">
                <setting help="Transport Management : Organize packs in your fleet, or carriers.">
                    <field name="module_dispatch_system" string="Transport Management System"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
